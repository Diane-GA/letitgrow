<%= month_calendar events: @tasks do |date, tasks| %>
<%= l(date, format: :default) %>


  <% tasks.each do |task| %>
    <div>
    <p>
      <%= task.name %> tes
      <%# <%= cl_image_tag(task.culture.photo.key,
      width: 400, height: 300, crop: :fill) %> %>
      <%= task.culture.name %>s
    </p>
    </div>
  <% end %>
<% end %>
